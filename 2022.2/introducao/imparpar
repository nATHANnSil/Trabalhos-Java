package introducao;
//

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/**/ 
		int intA = MyIO.readInt("Digite o primeiro numero:");
		
		int intB;
		intB = MyIO.readInt("Digite o segundo numero");
		
		int soma = intA+intB;
		
		if (soma%2==0) {
			MyIO.println("É par");
		} else 
			MyIO.println("é impar");
		
		MyIO.println(soma);

	}

}
